            <img src="control1_option3.png" alt="option 3"/>
            <img src="control1_option4.png" alt="option 4"/>
            <img src="control1_option5.png" alt="option 5"/>
            <img src="control1_option6.png" alt="option 6"/>
            <img src="control1_option7.png" alt="option 7"/>
            <img src="control1_option8.png" alt="option 8"/>
          </options>
        </phase>
      </phases>
    </protocol>
  </protocols>
</config>
